{
  "name": "promise",
  "private": true,
  "scripts": {
    "build": "poi --prod && electron-builder",
    "tsc": "tsc --outDir dist",
    "tsc:watch": "tsc --outDir dist -w",
    "dev": "cross-env NODE_ENV=development run-p dev:web tsc:watch dev:main -r",
    "dev:main": "electron --inspect=5858 dist/main/index.dev.js",
    "dev:web": "poi --serve",
    "lint": "eslint -f ./node_modules/eslint-friendly-formatter .",
    "lint:fix": "eslint -f ./node_modules/eslint-friendly-formatter --fix ."
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@poi/plugin-typescript": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-gaoding": "^1.0.2",
    "eslint-config-gaoding-vue": "^1.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "poi": "^12.4.2",
    "sass-loader": "^9.0.2",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-updater": "^4.3.1",
    "vue": "^3.0.0-beta.19",
    "vuex": "^4.0.0-beta.4"
  }
}
