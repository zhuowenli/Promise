{
  "name": "promise",
  "private": true,
  "scripts": {
    "build:web": "vue-cli-service build --mode development ./web/index.ts",
    "build:main": "electron-builder dist/main/index.js",
    "tsc": "tsc --outDir dist",
    "tsc:watch": "tsc --outDir dist -w",
    "dev": "cross-env NODE_ENV=development run-p dev:web tsc:watch dev:main -r",
    "dev:main": "electron --inspect=5858 dist/main/index.dev.js",
    "dev:web": "vue-cli-service serve --mode development ./web/index.ts --port 9090",
    "lint": "eslint -f ./node_modules/eslint-friendly-formatter .",
    "lint:fix": "eslint -f ./node_modules/eslint-friendly-formatter --fix ."
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-gaoding": "^1.0.2",
    "eslint-config-gaoding-vue": "^1.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^9.0.2",
    "typescript": "^3.2.1",
    "vue-cli-plugin-vue-next": "^0.1.3"
  },
  "dependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-updater": "^4.3.1",
    "monaco-editor": "^0.20.0",
    "normalize.css": "^8.0.1",
    "vue": "^3.0.0-beta.19",
    "vue-electron": "^1.0.6",
    "vuex": "^4.0.0-beta.4",
    "vuex-electron": "^1.0.3"
  }
}
